<?xml version="1.0" encoding="utf-8"?>
<Containers>
    <Date>2021-01-24</Date>
    <Beta>False</Beta>
    <Name>Startpage</Name>
    <Overview>
        A custom startpage based on SUI.
    </Overview>
    <Registry>https://registry.hub.docker.com/u/skippythesnake/startpage</Registry>
    <GitHub>https://github.com/SkippyTheSnake/Startpage</GitHub>
    <Repository>skippythesnake/startpage</Repository>
    <Support>https://github.com/SkippyTheSnake/Startpage</Support>
    <BindTime>true</BindTime>
    <Privileged>false</Privileged>
    <Networking>
        <Mode>host</Mode>
        <Publish>
            <Port>
                <HostPort>5003</HostPort>
                <ContainerPort>3000</ContainerPort>
                <Protocol>tcp</Protocol>
                <Description>WebUI</Description>
            </Port>
        </Publish>
    </Networking>
    <Environment>
        <Variable>
            <Name>HOST</Name>
            <Value></Value>
            <Description>Url of the host e.g: http://192.128.0.1:5003 or https://start.example.com</Description>
        </Variable>
    </Environment>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/startpage</HostDir>
            <ContainerDir>/app/data</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <WebUI>http://[IP]:[PORT:5003]</WebUI>
</Containers>